        "# 문제1\n",
        "# 반지름이 5인 원의 면적을 구하시오.\n",
        "# pi(원주율) 값은 3.141592로 계산합니다.\n",
        "\n",
        "radius = 5\n",
        "pi = 3.141592\n",
        "\n",
        "area = pi * radius ** 2\n",
        "\n",
        "print(area)"
